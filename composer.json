{
    "name": "michaelray/think-library",
    "description": "基于think-library做了一些扩展",
    "type": "think-extend",
    "license": "MIT",
    "authors": [
        {
            "name": "MichaelRay",
            "email": "michaelray@vip.qq.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "zoujingli/think-library": "5.1.*-dev",
		"ext-curl": "*",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4":{
            "MichaelRay\\ThinkLibrary\\":"src"
        },
        "files": [
            "src/common.php"
        ]
    },
    "extra": {
        "think-config": {
            "socket_log": "src/config/socket_log.php",
            "verification": "src/config/verification.php"
        }
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer"
        }
    }
}
